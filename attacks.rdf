		<attacks:likelihood_of_attack rdf:datatype="http://w3.org/2001/XMLSchema#string">high</attacks:likelihood_of_attack>
		<attacks:typical_severity rdf:datatype="http://w3.org/2001/XMLSchema#string">high</attacks:typical_severity>
		<attacks:prerequisites rdf:datatype="http://w3.org/2001/XMLSchema#string">relies_on_cookies, cookie_contains_sensitive_information, response_contains_cookie</attacks:prerequisites>
		<attacks:availability rdf:datatype="http://w3.org/2001/XMLSchema#string">none</attacks:availability>
		<attacks:confidentiality rdf:datatype="http://w3.org/2001/XMLSchema#string">read_data</attacks:confidentiality>
		<attacks:confidentiality_access_control_authorization rdf:datatype="http://w3.org/2001/XMLSchema#string">gain_privileges</attacks:confidentiality_access_control_authorization>
		<attacks:mitigations rdf:datatype="http://w3.org/2001/XMLSchema#string">use_input_validation_for_cookies, generate_and_validate_MAC_for_cookies, use_ssl/tls_to_protect_cookie_in_transit, web_server_implements_all_relevant_security_patches</attacks:mitigations>
	</rdf:Description>
		<rdf:Description rdf:about="#utilizing_RESTs_Trust_in_the_System_Resource_to_Obtain_Sensitive_Data">
		<rdf:type rdf:resource="#Attack"/>
